<odoo>
    <record id="action_window_sase_enfant_all" model="ir.actions.act_window">
        <field name="res_model">sase.enfant</field>
        <field name="view_mode">list,form</field>
        <field name="context">{'search_default_situation_ids': 1}</field>
    </record>
    <record id="action_window_sase_situation_all" model="ir.actions.act_window">
        <field name="res_model">sase.situation</field>
        <field name="view_mode">list,form</field>
    </record>
    <record id="action_window_sase_service_all" model="ir.actions.act_window">
        <field name="res_model">sase.service</field>
        <field name="view_mode">list,form</field>
    </record>
    <record id="action_window_sase_intervenant_all" model="ir.actions.act_window">
        <field name="res_model">sase.intervenant</field>
        <field name="view_mode">list,form</field>
    </record>
    <record id="action_window_sase_rapports_all" model="ir.actions.act_window">
        <field name="res_model">sase.rapport</field>
        <field name="view_mode">list</field>
    </record>

    <!-- MENU ITEMS -->
    <menuitem id="menu_sase" name="SASE" sequence="10">
        <menuitem id="menu_sase_enfant" name="Enfants" sequence="10" action="action_window_sase_enfant_all" />
        <menuitem id="menu_sase_situation" name="Situations" sequence="20" action="action_window_sase_situation_all" />
        <menuitem id="menu_sase_intervenant" name="Intervenants" sequence="30" action="action_window_sase_intervenant_all" />
        <menuitem id="menu_sase_rapports" name="Rapports" sequence="40" action="action_window_sase_rapports_all" />

        <menuitem id="menu_sase_service" name="Services mandants" sequence="70" action="action_window_sase_service_all" />

    </menuitem>

</odoo>